{
  "name": "lazlospizza-api",
  "version": "1.0.0",
  "description": "Lazlo's Pizza API",
  "scripts": {
    "start": "NODE_ENV=production node dist/server.js",
    "dev": "cross-env NODE_ENV=development nodemon",
    "build": "tsc && npx tsc-alias",
    "test": "jest --forceExit --detectOpenHandles",
    "lint": "eslint --ignore-path .eslintignore --ext .ts src/",
    "lint:fix": "yarn lint -- --fix",
    "typeorm": "cross-env NODE_ENV=development node --require ts-node/register ./node_modules/typeorm/cli.js",
    "wtf:mac": "rm -rf ./node_modules && yarn cache clean && yarn",
    "wtf:windows": "rd /s /q \"./node_modules\" && yarn cache clean && yarn"
  },
  "engines": {
    "node": ">10.14.2"
  },
  "license": "ISC",
  "dependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@typechain/hardhat": "^6.0.0",
    "async": "^3.2.0",
    "aws-sdk": "^2.1141.0",
    "axios": "^0.19.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "ethers": "^5.6.7",
    "express": "^4.17.1",
    "fs": "^0.0.1-security",
    "hardhat": "^2.9.6",
    "http": "0.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "memory-cache": "^0.2.0",
    "moment": "^2.29.1",
    "ncp": "^2.0.0",
    "node-fetch": "^2.6.0",
    "pg": "^8.0.3",
    "solidity-coverage": "^0.7.21",
    "ts-node": "^8.6.2",
    "typechain": "^8.0.0",
    "typeorm": "0.2.24",
    "typescript": "^4.0.5",
    "uuid": "^8.0.0",
    "winston": "^2.4.5"
  },
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/cors": "^2.8.7",
    "@types/crypto-js": "^4.0.1",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.163",
    "@types/merge-images": "^1.2.1",
    "@types/mocha": "^9.1.1",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.4",
    "@types/node": "^13.9.0",
    "@types/papaparse": "^5.2.4",
    "@types/pg": "^7.14.3",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.2",
    "nodemon": "^2.0.2",
    "prettier": "^2.0.4"
  }
}
